{
	"tags": {
		"allowUnknownTags": true
	},
	"opts": {
		"destination": "./build/documention"
	},
	"plugins": [
		"plugins/markdown"
		],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles": true
		},
		"path": "ink-docstrap",
		"theme": "cerulean",
		"navType": "inline",
		"linenums": true,
		"dateFormat": "YYYY, MMMM Do h:mm:ss a"
	}
}